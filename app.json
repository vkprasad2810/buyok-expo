{
  "expo": {
    "name": "buyok-expo",
    "slug": "buyok-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },

    "splash": {
      "image": "./assets/images/react-logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#35393C"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vkprasad2810.buyokexpo"
    },
    "androidStatusBar": {
      "backgroundColor": "#1F2326",
      "translucent": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2c27cb02-011b-4a3f-b1c7-1de5dee0a113"
      }
    },
    "owner": "vkprasad2810"
  }
}
